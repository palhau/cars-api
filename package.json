{
  "private": true,
  "name": "adere-middleware",
  "version": "0.0.1",
  "description": "Cars middleware",
  "scripts": {
    "commit": "git-cz",
    "dev": "nodemon index.ts",
    "lint": "npm run lint:ts",
    "lint:js": "run-script-os",
    "lint:js:win32": "eslint **/*.{js,ts} --fix --color",
    "lint:js:darwin:linux": "eslint '**/*.{js,ts}' --fix --color",
    "lint:js:default": "eslint **/*.{js,ts} --fix --color",
    "prod": "node index.ts set NODE_ENV=production",
    "start": "node index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickfeitosa/adere-middleware/src/master"
  },
  "keywords": [
    "backend",
    "js",
    "ts",
    "node"
  ],
  "author": "Alan Vieira Palhau<alanvieirapalhau@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patrickfeitosa/adere-middleware/issues"
  },
  "homepage": "https://github.com/patrickfeitosa/adere-middleware#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "isomorphic-fetch": "^3.0.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "run-script-os": "^1.1.6",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@": ".",
    "@APP": "./src/app",
    "@Config": "./src/configuration",
    "@Controllers": "./src/controllers",
    "@Middlewares": "./src/middlewares",
    "@Routes": "./src/routes",
    "@Server": "./src/server"
  },
  "engines": {
    "node": ">=14.15.1",
    "npm": ">=6.14.8"
  }
}
